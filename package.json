{
  "name": "mybooklist",
  "version": "0.0.0",
  "description": "Keep track of your books",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production browserify main.js | uglifyjs -cm > public/bundle.js",
    "start": "node server.js",
    "watch": "watchify main.js -o public/bundle.js -dv"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lw7360/mybooklist.git"
  },
  "author": "Larry Wu <lw7360@gmail.com> (http://larrywu.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lw7360/mybooklist/issues"
  },
  "homepage": "https://github.com/lw7360/mybooklist#readme",
  "dependencies": {
    "babelify": "^6.1.3",
    "browserify": "^10.2.6",
    "ecstatic": "~0.8.0",
    "express": "^4.13.3",
    "pg": "^4.4.2",
    "react": "~0.13.3",
    "uglify-js": "^2.4.24",
    "watchify": "^3.2.3"
  }
}
