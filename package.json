  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lw7360/mybooklist/issues"
  },
  "homepage": "https://github.com/lw7360/mybooklist#readme",
  "dependencies": {
    "@lw7360/react-progressbar.js": "^0.1.2",
    "babel": "^5.8.23",
    "babelify": "^6.1.3",
    "body-parser": "^1.14.1",
    "browserify": "^10.2.6",
    "cookie-parser": "^1.4.0",
    "express": "^4.13.3",
    "greatreads": "0.0.1",
    "immutable": "^3.7.5",
    "node-uuid": "^1.4.3",
    "passport": "^0.3.0",
    "passport-local": "^1.0.0",
    "pg": "^4.4.2",
    "react": "~0.13.3",
    "react-internal-nav": "^1.0.0",
    "react-redux": "^3.0.0",
    "redux": "^3.0.2",
    "store": "^1.3.17",
    "uglify-js": "^2.4.24",
    "watchify": "^3.2.3",
    "xhr": "^2.1.0"
  },
  "devDependencies": {
    "redux-devtools": "^2.1.5",
    "semistandard": "*"
  }
