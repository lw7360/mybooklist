{
  "name": "mybooklist",
  "version": "0.0.0",
  "description": "Keep track of your books",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production browserify main.js | uglifyjs -cm > public/bundle.js",
    "start": "babel-node server.js",
    "watch": "watchify -g main.js -o public/bundle.js -dv",
    "test": "semistandard"
  },
